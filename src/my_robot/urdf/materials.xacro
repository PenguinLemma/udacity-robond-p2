<?xml version="1.0"?>

<robot xmlns:xacro="http://www.ros.org/wiki/xacro">


    <!-- First palette -->
    <!-- https://coolors.co/fe938c-e6b89c-ead2ac-9cafb7-4281a4 -->
    <material name="tulip">
        <color rgba="${254/255} ${147/255} ${140/255} 1" />
    </material>

    <material name="pale_gold">
        <color rgba="${230/255} ${184/255} ${156/255} 1" />
    </material>

    <material name="desert_sand">
        <color rgba="${234/255} ${210/255} ${172/255} 1" />
    </material>

    <material name="pewter_blue">
        <color rgba="${156/255} ${175/255} ${183/255} 1" />
    </material>

    <material name="steel_blue">
        <color rgba="${66/255} ${129/255} ${164/255} 1" />
    </material>


    <!-- Second palette -->
    <!-- https://coolors.co/5bc0eb-fde74c-9bc53d-e55934-fa7921 -->
    <xacro:macro name="material_blue_jeans">
        <visual>
            <material>
                <ambient>${91/255} ${192/255} ${235/255} 1</ambient>
                <diffuse>${91/255} ${192/255} ${235/255} 1</diffuse>
                <specular>0.1 0.1 0.1 0.1 1</specular>
            </material>
        </visual>
    </xacro:macro>

